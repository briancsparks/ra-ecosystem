  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach by Process ID",
      "processId": "${command:PickProcess}",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },


    {
      "type": "node",
      "request": "launch",
      "name": "Debug current file using run-anywhere.bin_file",
      "program": "${workspaceFolder}/packages/run-anywhere/bin-file.js",
      "args": ["one-file", "${file}"],
      "cwd": "C:/Users/sparksb/dev/bcs/ra-ecosystem/apps/lambda-net",
      "internalConsoleOptions": "openOnSessionStart",
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug current file with Run-Anywhere",
      "program": "${workspaceFolder}/packages/run-anywhere/bin.js",
      "args": ["invoke", "${file}"],
      "cwd": "C:/Users/sparksb/dev/bcs/ra-ecosystem/apps/lambda-net",
      "internalConsoleOptions": "openOnSessionStart",
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug current file using run-anywhere/bin3.js",
      "program": "${workspaceFolder}/packages/run-anywhere/bin3.js",
      "args": ["invoke", "${file}"],
      "cwd": "C:/Users/sparksb/dev/bcs/ra-ecosystem/apps/lambda-net",
      "internalConsoleOptions": "openOnSessionStart",
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug current file using quick-net,js",
      "program": "${workspaceFolder}/packages/quick-net/bin/quick-net.js",
      "args": ["upsertInstance", "--useful=workstationb"],
      "cwd": "C:/Users/sparksb/dev/bcs/ra-ecosystem/apps/lambda-net",
      "internalConsoleOptions": "openOnSessionStart",
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug current file using run-anywhere/platform/app/workstation-cli.js",
      "program": "${workspaceFolder}/packages/run-anywhere/platform/app/workstation-cli.js",
      "args": ["${file}"],
      "cwd": "C:/Users/sparksb/dev/bcs/ra-ecosystem/apps/lambda-net",
      "internalConsoleOptions": "openOnSessionStart",
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug deploy.js run-anywhere",
      "program": "${workspaceFolder}/packages/run-anywhere/bin.js",
      "args": ["invoke", "${workspaceFolder}/packages/quick-net/commands/lambda-deploy/deploy.js", "deployLambda", "--stage=dev", "--name=lambda-net", "--class-b=21", "--sgs=wide", "--debug", "--log-api"],
      "cwd": "C:/Users/sparksb/dev/bcs/ra-ecosystem/apps/lambda-net",
      "internalConsoleOptions": "openOnSessionStart",
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Current File",
      "program": "${file}",
      "internalConsoleOptions": "openOnSessionStart",
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Current File - change my args",
      "program": "${file}",
      "args": ["snake"],
      "internalConsoleOptions": "openOnSessionStart",
    },
    {
      "type": "node",
      "request": "launch",
      "name": "ra invoke",
      "program": "${workspaceFolder}/ra.js",
      "args": [ "invoke", "lib/loading.js", "testHook", "--a", "--verbose" ],
      "cwd": "C:/Users/sparksb/dev/bcs/ra-ecosystem",
    }
  ]
}
