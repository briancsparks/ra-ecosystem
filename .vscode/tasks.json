    },
    "command":["node ${workspaceFolder}/./packages/quick-net/bin/quick-net.js buildLayer --useful=lambdanet"],
  },
  {
    "label": "deploy lambda (2)",
    "type": "shell",
    "options": {
      "cwd": "${workspaceFolder}/apps/lambda-net"
    },
    "command":["node ${workspaceFolder}/./packages/quick-net/bin/quick-net.js deployLambda --useful=dev"],
  }
